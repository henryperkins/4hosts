# Four Hosts Research API Environment Variables
# Copy this to .env and fill in your actual API keys

# Search APIs
GOOGLE_SEARCH_API_KEY=your_google_custom_search_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here
BING_SEARCH_API_KEY=your_bing_search_api_key_here

# Domain Authority APIs (Optional)
MOZ_API_KEY=your_moz_api_key_here
MOZ_SECRET_KEY=your_moz_secret_key_here

# PubMed API (Optional - for enhanced rate limits)
PUBMED_API_KEY=your_pubmed_api_key_here

# Redis Configuration
REDIS_URL=redis://localhost:6379

# OpenAI/Anthropic APIs (for future LLM integration)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Development Settings
DEBUG=true
LOG_LEVEL=INFO